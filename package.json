{
  "name": "@privyid/eslint-config-js",
  "version": "5.0.0",
  "description": "Eslint shareable config for Javascript",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint -c ./index.js ./*.js",
    "fix": "eslint -c ./index.js ./*.js --fix",
    "test:lint": "bash ./script/lint.sh",
    "test:fix": "bash ./script/fix.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/privy-open-source/eslint-config-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": {
    "name": "Ade Novid",
    "email": "adenvt@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/privy-open-source/eslint-config-js/issues"
  },
  "homepage": "https://github.com/privy-open-source/eslint-config-js#readme",
  "devDependencies": {
    "babel-eslint": ">=10.1.0",
    "eslint": ">=7.32.0",
    "eslint-config-standard": ">=16.0.3",
    "eslint-plugin-import": ">=2.24.0",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-promise": ">=5.1.0",
    "eslint-plugin-standard": ">=5.0.0",
    "eslint-plugin-unicorn": ">=35.0.0",
    "eslint-plugin-varspacing": "^1.2.2",
    "eslint-plugin-vue": ">=7.16.0"
  },
  "peerDependencies": {
    "babel-eslint": ">=10.0.0",
    "eslint": ">=6.8.0",
    "eslint-config-standard": ">=14.0.0",
    "eslint-plugin-import": ">=2.18.0",
    "eslint-plugin-node": ">=11.0.0",
    "eslint-plugin-promise": ">=4.0.0",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-unicorn": ">=15.0.0",
    "eslint-plugin-vue": ">=6.0.0"
  }
}
